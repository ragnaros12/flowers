@model Flower

<form asp-action="Add" method="post">
    <input type="text" asp-for="Name" placeholder="Введите имя"/>
    <input type="text" asp-for="Description" placeholder="Введите описание" />
    <input type="number" asp-for="Price" placeholder="Введите цену" />
    <input type="number" asp-for="Height" placeholder="Введите высоту" />
    <input type="hidden" asp-for="Image" value="images/poins.jpeg" />

    <input type="submit"/>
</form>
